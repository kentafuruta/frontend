{
  "name": "frontend",
  "version": "1.0.0",
  "description": "npm-scripts, gulp, ejs, sass, babel build tool.",
  "main": "index.js",
  "style": [
    "./node_modules/tipso/src/tipso.min.css"
  ],
  "browser": {
    "tipso": "./node_modules/tipso/src/tipso.js"
  },
  "scripts": {
    "start": "npm run watch",
    "install": "npm run normalize && npm run vendor && npm run build",
    "build": "npm-run-all build:*",
    "build:html": "gulp html",
    "build:js": "gulp js",
    "build:css": "gulp css",
    "build:sprite": "gulp sprite",
    "build:iconfont": "gulp iconfont",
    "clean": "rimraf **/.DS_Store",
    "font": "cpx ./src/assets/font/* ./public/assets/font/",
    "normalize": "cpx ./node_modules/normalize.css/normalize.css ./public/assets/css/",
    "release": "npm-run-all -s clean -p release:*",
    "release:gulp": "gulp release",
    "server": "browser-sync start -s ./public -f ./public/*",
    "vendor": "mkdirp ./public/assets/js && browserify ./src/assets/js/vender/vender.js -o ./public/assets/js/vender.js -p [ parcelify -o ./public/assets/css/vender.css ] --debug",
    "watch": "npm-run-all -p watch:* server",
    "watch:gulp": "gulp watch",
    "watch:font": "onchange \"./src/assets/font/*\" -- npm run font",
    "watch:normalize": "onchange ./node_modules/normalize.css/normalize.css -- npm run normalize",
    "watch:vender": "watchify ./src/assets/js/vender/vender.js -o ./public/assets/js/vender.js -p [ parcelify -o ./public/assets/css/vender.css ] --debug"
  },
  "keywords": [
    "frontend",
    "gulp",
    "npm-scripts",
    "ejs",
    "sass",
    "babel"
  ],
  "author": "Keiji Miyake",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-preset-latest": "^6.16.0",
    "browser-sync": "^2.17.3",
    "browserify": "^13.1.1",
    "cpx": "^1.5.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-consolidate": "^0.2.0",
    "gulp-ejs": "^2.2.1",
    "gulp-iconfont": "^8.0.1",
    "gulp-imagemin": "^3.0.3",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sass-lint": "^1.3.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp.spritesmith": "^6.2.1",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "licensify": "^3.1.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^3.1.1",
    "onchange": "^3.0.2",
    "parcelify": "^2.2.0",
    "require-dir": "^0.3.1",
    "rimraf": "^2.5.4",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "lodash": "^4.16.6",
    "moment": "^2.16.0",
    "normalize.css": "^5.0.0",
    "tipso": "^1.0.8"
  }
}
