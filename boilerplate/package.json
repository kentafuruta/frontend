{
  "name": "frontend",
  "version": "1.0.0",
  "description": "npm-scripts, sass, babel build tool.",
  "main": "index.js",
  "style": [
    "./node_modules/tipso/src/tipso.min.css"
  ],
  "browser": {
    "tipso": "./node_modules/tipso/src/tipso.js"
  },
  "scripts": {
    "start": "npm run watch",
    "install": "npm run copy && npm run build",
    "build": "npm-run-all build:*",
    "build:js": "babel ./src/js/module/ -d ./src/js/ --source-maps",
    "build:css": "npm run sass && npm run autoprefixer",
    "build:sprite": "gulp sprite",
    "build:iconfont": "gulp iconfont",
    "build:vender": "browserify ./src/js/vender/vender.js -o ./src/js/vender.js -p [ parcelify -o ./src/css/vender.css ] --debug",
    "watch": "npm-run-all -p watch:* server",
    "watch:js": "babel -w ./src/js/module/ -d ./src/js/ --source-maps",
    "watch:css": "npm run sass:watch && autoprefixer:watch",
    "watch:gulp": "gulp watch",
    "watch:vender": "watchify ./src/js/vender/vender.js -o ./src/js/vender.js -p [ parcelify -wo ./src/css/vender.css ] --debug",
    "release": "npm-run-all -s clean:release copy:release -p release:*",
    "release:css": "npm run sass:release && npm run autoprefixer:release",
    "release:js": "mkdirp ./release/js/ && babel ./src/js/ -d ./release/js/",
    "release:image": "gulp image",
    "release:font": "cpx \"./src/font/*\" ./release/font/",
    "autoprefixer": "postcss -u autoprefixer --autoprefixer.browsers \"> 5%, ie 8\" -r ./src/css/style.css",
    "autoprefixer:watch": "postcss -w -u autoprefixer --autoprefixer.browsers \"> 5%, ie 8\" -r ./src/css/style.css",
    "autoprefixer:release": "postcss -u autoprefixer --autoprefixer.browsers \"> 5%, ie 8\" -r ./release/css/style.css",
    "clean:release": "rimraf ./release/",
    "copy": "cpx ./node_modules/normalize.css/normalize.css ./src/css/",
    "copy:release": "cpx \"./src/*\" ./release/ && cpx ./node_modules/normalize.css/normalize.css ./release/css/",
    "sass": "node-sass --source-map true --output-style expanded --indent-type space --indent-width 2 -o ./src/css/ ./src/sass/",
    "sass:watch": "node-sass -w --source-map true --output-style expanded --indent-type space --indent-width 2 -o ./src/css/ ./src/sass/",
    "sass:release": "node-sass --output-style compressed -o ./release/css/ ./src/sass/",
    "server": "browser-sync start -s ./ -f ./src/* --startPath ./src/",
    "jquery": "cpx \"./node_modules/jquery/dist/jquery.min.js\" ./src/js/"
  },
  "keywords": [
    "frontend",
    "npm-scripts",
    "sass",
    "babel"
  ],
  "author": "Keiji Miyake",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "browser-sync": "^2.17.3",
    "browserify": "^13.1.1",
    "cpx": "^1.5.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-consolidate": "^0.2.0",
    "gulp-iconfont": "^8.0.1",
    "gulp-imagemin": "^3.0.3",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp.spritesmith": "^6.2.1",
    "imagemin": "^5.2.2",
    "imagemin-cli": "^3.0.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "licensify": "^3.1.2",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.10.1",
    "npm-run-all": "^3.1.1",
    "onchange": "^3.0.2",
    "parcelify": "^2.2.0",
    "postcss-cli": "^2.6.0",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "require-dir": "^0.3.1",
    "rimraf": "^2.5.4",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "lodash": "^4.16.6",
    "normalize.css": "^5.0.0"
  }
}
